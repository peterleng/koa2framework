{
  "name": "koa2framework",
  "version": "1.0.0",
  "description": "koa2基本框架，可拿起来直接搭建项目使用",
  "main": "app.js",
  "scripts": {
    "start": "set NODE_ENV=production && node --use_strict ./bootstrap/app.js",
    "start_dev": "set NODE_ENV=development && ./node_modules/.bin/nodemon ./bootstrap/app.js",
    "test": "set NODE_ENV=test && echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "koa",
    "nodejs"
  ],
  "author": "lengyufeng",
  "license": "MIT",
  "dependencies": {
    "ejs": "^2.5.7",
    "koa": "^2.4.1",
    "koa-bodyparser": "^4.2.0",
    "koa-generic-session": "^2.0.0",
    "koa-isajax": "^1.0.5",
    "koa-jsonp": "^2.0.2",
    "koa-logger": "^3.1.0",
    "koa-multer": "^1.0.2",
    "koa-mysql-session": "0.0.2",
    "koa-redis": "^3.1.1",
    "koa-router": "^7.2.1",
    "koa-send": "^4.1.1",
    "koa-static": "^4.0.1",
    "koa-views": "^6.1.1",
    "moment": "^2.19.2",
    "mysql": "^2.15.0",
    "mysql2": "^1.4.2",
    "sequelize": "^4.22.5"
  },
  "devDependencies": {
    "nodemon": "^1.12.1"
  }
}
